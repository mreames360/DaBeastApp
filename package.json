    "method-override": "3.0.0",
    "mongoose": "7.8.4",
    "morgan": "1.10.1",
    "serve-favicon": "2.5.0",
    "serve-static": "1.16.1"
  },
  "engines": {
    "npm": ">=9.0.0",
    "node": ">=18.0.0"
  },
  "overrides": {
    "ejs@<=3.1.10": "3.1.10",
    "mongoose@<=7.8.4": "7.8.4",
    "body-parser@<=1.20.3": "1.20.3",
    "express@<=4.20.0": "4.20.0",
    "send@<=0.19.0": "0.19.0",
    "serve-static@<=1.16.0": "1.16.0",
    "path-to-regexp@<=0.1.12": "0.1.12",
    "cookie@<=0.7.0": "0.7.0",
