{
  "name": "dabeastapp",
  "version": "0.0.2",
  "description": "An intentionally vulnerable todo list application",
  "private": true,
  "homepage": "https://tigerdojo.io/",
  "repository": {
    "type": "git",
    "url": "https://github.com/tiger-dojo/DaBeastApp/"
